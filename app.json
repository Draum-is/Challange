{
  "expo": {
    "name": "Phone Challenge",
    "slug": "challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F4F7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.challenge"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F2F4F7"
      },
      "package": "com.yourname.challenge"
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "plugins": []
  }
}


